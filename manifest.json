  "browser_action": {
    "default_icon": {
      "19": "/images/icons/icon_32x32.png"
    },
    "default_title": "Roblox+"
  },
  "homepage_url": "https://www.roblox.com/my/account?tab=rplus",
  "permissions": [
    "alarms",
    "gcm",
    "contextMenus",
    "webRequest",
    "webRequestBlocking",
    "declarativeNetRequest",
    "tts",
    "notifications",
    "storage",
    "https://*.roblox.com/*",
    "https://*.rbxcdn.com/*",
    "https://*.roblox.plus/*"
  ],
  "web_accessible_resources": [
    "*.css",
    "*.svg",
    "*.png",
    "*.html",
    "*.js",
    "jquery.min.map",
    "/images/icons/icon_32x32.png",
    "https://*.roblox.com/*",
    "https://*.rbxcdn.com/*",
    "https://*.roblox.plus/*"
  ],
  "manifest_version": 2,
  "incognito": "split"
